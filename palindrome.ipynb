    "def pal(n):\n",
    "    num=n%10\n",
    "    nn=n//10\n",
   //update
    
    "    while nn>0:\n",
    "        num*=10\n",
    "        num+=nn%10\n",
    "    return num==n\n",
    "    \n",
    "def largePalinfrome(n):\n",
    "    val=0\n",
    "    i=10**n-1\n",
    "    j=i\n",
    "    while n>=10**(n-1) and i*(10**n-1)>val:\n",
    "        \n",
    "        j=10**n-1\n",
    "    while j>=10**(n-1) and i*j>val:\n",
    "        if pal(i*j):\n",
    "            val=i*j\n",
    "            val=i*j\n",
    "            j-=1\n",
    "            i-=1\n",
    "            return val"
   ]
